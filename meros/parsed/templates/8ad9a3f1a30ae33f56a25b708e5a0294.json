{"components/category/side-shop-by-brand":"<div class=\"sidebarBlock\" id=\"sideShopByBrands\">\r\n    <h5 class=\"side-module-heading\"><span>{{lang 'brand.label'}}</span></h5>\r\n    <ul class=\"navList\">\r\n        {{#each shop_by_brand}}\r\n            <li>\r\n                <a href=\"{{url}}\">{{name}}</a>\r\n            </li>\r\n        {{/each}}\r\n        <li>\r\n            <a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a>\r\n        </li>\r\n    </ul>\r\n</div>","components/common/footer":"<footer id=\"footer-wrapper\">\r\n    <section id=\"main-footer\">\r\n        <div class=\"container\">\r\n            <div class=\"row\">\r\n                <div class=\"col-1 col-sm-2\">\r\n                    <h5>Further Info.</h5>\r\n                    <ul>\r\n                        <li><a href=\"/blog/\">Blog</a></li>\r\n                        <li><a href=\"/about-us/\">About Us</a></li>\r\n                        <li><a href=\"/rss-syndication/\">RSS Syndication</a></li>\r\n                        <li><a href=\"/shipping-returns/\">Shipping & Returns</a></li>\r\n                       <!-- <li><a href=\"/theme-styles/\">Theme Styles</a></li>-->\r\n                        <li><a href=\"/sitemap/\">Sitemap</a></li>\r\n                        <li><a href=\"/contact-us/\">Contact Us</a></li>\r\n                        <li><a href=\"#\">Testimonials</a></li>\r\n                    </ul>\r\n                </div>\r\n                <div class=\"col-2 col-sm-2\">\r\n                    <h5>Customer Service</h5>\r\n                    <ul>\r\n                        <li><a href=\"#\">Help & FAQs</a></li>\r\n                        <li><a href=\"#\">Terms of Conditions</a></li>\r\n                        <li><a href=\"#\">Privacy Policy</a></li>\r\n                        <li><a href=\"#\">Online Returns Policy</a></li>\r\n                        <!--\r\n                        <li><a href=\"#\">Rewards Program</a></li>\r\n                        <li><a href=\"#\">Rebate Center</a></li>\r\n                        <li><a href=\"#\">Partners</a></li>\r\n                        -->\r\n                    </ul>\r\n                </div>\r\n                <div class=\"col-3 col-sm-2\">                   \r\n                    <h5>Shop</h5>\r\n                    <ul>\r\n                        {{#each categories}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n                <div class=\"col-4 col-sm-2\"> \r\n                    <h5>Top Brands</h5>\r\n                    {{#if shop_by_brand}}\r\n                        {{> components/category/side-shop-by-brand}}\r\n                    {{/if}}\r\n                </div>\r\n                <div class=\"col-5 col-sm-4\">\r\n                    {{#if settings.show_newsletter_box}} {{> components/common/subscription-form}} {{/if}}\r\n                    {{> components/common/social-links}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n    <div class=\"footerInfo\">\r\n        <div class=\"container\">\r\n            <p>Need help? We're available at 800-996-4340 • Email us at <a href=\"mailto:support@dealbeds.com\" title=\"support@dealbeds.com\">support@dealbeds.com</a> or <a href=\"#\" title=\"Live Help\" class=\"link\">Live Help</a></p>\r\n        </div>\r\n    </div>\r\n    <div id=\"bottom-footer\">\r\n        <div class=\"container\">\r\n            <div class=\"row\">\r\n                <div class=\"footer-copyright-{{theme_settings.copyright_alignment}} col-sm-6\">\r\n                    {{#if theme_settings.show_copyright_footer}}\r\n                        <div class=\"footer-copyright\">\r\n                            <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} </p>\r\n                        </div>\r\n                    {{/if}}\r\n\r\n                    {{#if theme_settings.show_powered_by}}\r\n                        <div class=\"powered-by\">\r\n                            <p>Powered by <a href=\"http://bigcommerce.com\">BigCommerce</a>.</p>\r\n                        </div>\r\n                    {{/if}}\r\n\r\n                    {{#if theme_settings.show_template_by}}\r\n                        <div class=\"template-by\">\r\n                            <p>BigCommerce Templates by <a href=\"http://themevale.com.com\" target=\"_blank\" rel=\"nofollow\">ThemeVale.com</a></p>\r\n                        </div>\r\n                    {{/if}}\r\n                </div>\r\n                <div class=\"footer-payment col-sm-6\">\r\n                    <p>\r\n                        <a title=\"Skrill\" href=\"#\"><img src=\"/product_images/uploaded_images/skrill.png\" alt=\"Skrill\"></a>\r\n                        <a title=\"Paypal\" href=\"#\"><img src=\"/product_images/uploaded_images/paypal.png\" alt=\"Paypal\"></a>\r\n                        <a title=\"Visa\" href=\"#\"><img src=\"/product_images/uploaded_images/visa.png\" alt=\"Visa\"></a>\r\n                        <a title=\"Express\" href=\"#\"><img src=\"/product_images/uploaded_images/american.png\" alt=\"Express\"></a>\r\n                        <a title=\"Master Card\" href=\"#\"><img src=\"/product_images/uploaded_images/mastercard.png\" alt=\"Master Card\"></a>\r\n                    </p>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n</footer>\r\n\r\n<a href=\"javascript:void(0);\" id=\"back-to-top\">Top</a>\r\n\r\n{{#if theme_settings.halothemes_newsletter_popup}}\r\n{{> components/halothemes/newsletter-popup}}\r\n{{/if}}","components/common/social-links":"{{#if social_media}}\r\n    <div class=\"social_media\">\r\n    <h5>Social</h5>\r\n    <ul class=\"social-icons\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                {{#if display_name '===' 'google+'}}\r\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\">\r\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\r\n                    </a>\r\n                {{else}}\r\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\r\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n    <div class=\"social_icon HL-facebooklike\" style=\"display:;\">       \r\n        <!-- FaceBook Like Button Code -->\r\n        <div class=\"fb-like\" data-href=\"https://developers.facebook.com/docs/plugins/\" data-layout=\"button_count\" data-action=\"like\" data-show-faces=\"true\"></div>\r\n             <div id=\"fb-root\"></div>\r\n        <script>(function(d, s, id) {            \r\n            var js, fjs = d.getElementsByTagName(s)[0];\r\n            if (d.getElementById(id)) return;\r\n            js = d.createElement(s); js.id = id;\r\n            js.src = \"//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.0\";\r\n            fjs.parentNode.insertBefore(js, fjs);\r\n            }(document, 'script', 'facebook-jssdk'));            \r\n        </script>                    \r\n    </div>\r\n    </div>\r\n{{/if}}\r\n","components/common/subscription-form":"<div id=\"footerSubcribeForm\">\r\n    <h5>{{lang 'newsletter.subscribe'}}</h5>\r\n        <p>{{lang 'newsletter.subscribe_intro'}}</p>\r\n    <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n            <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n            <input type=\"hidden\" name=\"check\" value=\"1\">\r\n            <div class=\"form-field\">\r\n                <div class=\"form-wrap\">\r\n                    <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                    <input class=\"btn btn-subcribe\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n                </div>\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n</div>","components/halothemes/newsletter-popup":"<!-- Begin Halo Newsletter Popup -->\r\n<div id=\"halo-newsletter-popup\" class=\"hide\">\r\n    <div class=\"popup-overlay\"></div>\r\n    <div class=\"newsletter-popup-container\">\r\n        <div class=\"newsletter-popup-inner\">\r\n            <button title=\"Close\" type=\"button\" data-close-newsletter-popup>×</button>\r\n            <div class=\"newsletter-left\">\r\n                <h2 class=\"newsletter-heading\">Signup for our emails</h2>\r\n                <p class=\"newsletter-sub-heading\">Receive our latest updates about our products and promotions.</p>\r\n                <div id=\"popupSubcribeForm\">\r\n                    <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n                        <fieldset class=\"form-fieldset\">\r\n                            <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n                            <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n                            <input type=\"hidden\" name=\"check\" value=\"1\">\r\n                            <div class=\"form-field\">\r\n                                <div class=\"form-wrap\">\r\n                                    <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                                    <input class=\"btn btn-primary btn-subcribe\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n                                </div>\r\n                            </div>\r\n                        </fieldset>\r\n                    </form>\r\n                </div>\r\n                <p class=\"newsletter-message\">We promise to only send you good things.</p>\r\n                <div class=\"black-icons\">\r\n                    {{> components/common/social-links}}\r\n                </div><div class=\"HL_check\">\r\n                    <input class=\"CheckBox\" id=\"HL_checked\" name=\"check\" value=\"\" onclick=\"\" type=\"checkbox\" />\r\n                    <label>Don’t show again!</label>\r\n                </div>\r\n            </div>\r\n            <div class=\"newsletter-right\">\r\n                <img src=\"../product_images/uploaded_images/img-email.jpg\" alt=\"\">\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n<!-- End Halo Newsletter Popup -->\r\n"}
